{
  "name": "nextjs13-sanity-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local npx prisma generate && next dev",
    "build": "dotenv -e .env.local npx prisma generate --accept-data-loss && dotenv -e .env.local npx prisma db push --accept-data-loss && next build",
    "start": "next start",
    "lint": "next lint",
    "prismaDev": "dotenv -e .env.local npx prisma migrate dev ",
    "prismaMigrate": "dotenv -e .env.local npx prisma migrate deploy",
    "prismaGenerate": "dotenv -e .env.local npx prisma generate",
    "prismaStudio": "dotenv -e .env.local npx prisma studio",
    "prismaReset": "dotenv -e .env.local npx prisma migrate reset --force",
    "prismaDBPush": "dotenv -e .env.local npx prisma db push",
    "prepare": "husky install"
  },
